{
  "name": "brobotjs",
  "version": "1.0.0",
  "description": "A chat bot and API interface",
  "main": "index.js",
  "proxy": "http://localhost:3000",
  "mocha": {
    "requires": [
      "ts-node/register"
    ],
    "reporter": "progress"
  },
  "scripts": {
    "build": "npm run build:pre && tsc -p . && npm run build:post",
    "build:pre": "rimraf build",
    "build:post": "cpx package.json ./build/ && cpx ormconfig.js ./build/ && cpx vocabulary.json ./build/",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec ts-node src/server/index.ts",
    "build-all": "npm run build && npm run build-frontend",
    "build-frontend": "npm run build-frontend:pre && webpack --mode production && npm run build-frontend:post",
    "build-frontend:pre": "rimraf ./build/dist",
    "build-frontend:post": "cpx \"./app/public/*.{ico,png,json}\" ./build/dist",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "npm run build && ts-node ./node_modules/typeorm/cli.js migration:run",
    "start": "node ./build/src/server/index.js",
    "start-dev": "npm run build:live",
    "start-frontend": "webpack-dev-server --mode development --open --hot",
    "test": "npm run test:frontend && npm run test:backend",
    "test:backend": "cross-env VOCABULARY_FILE=./testVocabulary.json ts-mocha -p tsconfig.json src/**/*.test.ts --exit",
    "test:frontend": "ts-mocha -p ./tsconfig.app.json app/**/*.test.{ts,tsx} --require ignore-styles --require ./app/setup.js --exit",
    "test:watch": "cross-env VOCABULARY_FILE=./testVocabulary.json ts-mocha -p tsconfig.json test/**/*.test.ts --watch --watch-extensions ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GovernmentHack/BroBotJS.git"
  },
  "author": "GovernmentHack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GovernmentHack/BroBotJS/issues"
  },
  "homepage": "https://github.com/GovernmentHack/BroBotJS#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "discord.js": "^11.5.1",
    "express": "^4.17.1",
    "mysql": "^2.17.1",
    "node-fetch": "^2.6.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/d3-sankey": "^0.11.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/expect": "^1.20.4",
    "@types/express": "^4.16.1",
    "@types/fetch-mock": "^7.3.1",
    "@types/jsdom": "^12.2.4",
    "@types/material-ui": "^0.21.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/sinon": "^7.0.12",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "d3-sankey": "^0.12.3",
    "d3-sankey-circular": "^0.34.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fetch-mock": "^7.3.9",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.1.1",
    "mocha": "^10.2.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.3.1",
    "sinon": "^7.3.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "ts-mocha": "^6.0.0",
    "ts-mockito": "^2.3.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
